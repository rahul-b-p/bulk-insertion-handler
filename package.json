{
  "name": "bulk-insertion-handler",
  "version": "1.0.0",
  "description": "to handle bulk insertions using any apis",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "start": "node dist/index.js",
    "dev": "nodemon --exec tsx src/index.ts",
    "docker:up": "docker compose up app --build -d",
    "docker:down": "docker compose down app",
    "docker:dev:up": "docker compose up app-dev --build -d",
    "docker:dev:down": "docker compose down app-dev"
  },
  "keywords": [
    "node",
    "express",
    "csv",
    "json",
    "upload",
    "api"
  ],
  "author": "Rahul B P",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.2.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "multer": "^2.0.2",
    "zod": "^4.3.6"
  }
}
